{
  "name": "breav-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Breav backend service",
  "main": "src/server.js",
  "scripts": {
    "db:init": "node -r module-alias/register -r dotenv/config ./scripts/db/init.js",
    "start": "node -r module-alias/register -r dotenv/config ./src/server.js",
    "predev": "npm run db:init",
    "dev": "nodemon -r module-alias/register -r dotenv/config ./src/server.js",
    "debug": "NODE_ENV=development nodemon --inspect ./src/server.js",
    "migrate": "sequelize db:migrate",
    "migrate:test": "SET NODE_ENV=test&&sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo:all",
    "prepare": "scripts/prepare.sh",
    "lint": "eslint --cache .",
    "test": "NODE_ENV=test jest --forceExit --verbose --detectOpenHandles",
    "test:unit": "npm run test tests/unit",
    "test:integration": "npm run test tests/integration",
    "test:coverage": "npm run test -- --coverage",
    "seed:template": "npx sequelize-cli db:seed:undo --seed 20210317091140-add-templates.js && npx sequelize-cli db:seed --seed 20210317091140-add-templates.js"
  },
  "engines": {
    "node": ">=14.17.2"
  },
  "keywords": [
    "breav"
  ],
  "author": "Chiwuzoh Daniel (https://chiwuzoh.com.ng/)",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.3",
    "cache-manager-redis-store": "^3.0.1",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "moment-timezone": "^0.5.38",
    "passport": "^0.6.0",
    "pg": "^8.8.0",
    "rate-limiter-flexible": "^2.4.1",
    "sequelize": "^6.25.5",
    "sequelize-cli": "^6.5.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-extended": "^3.1.0",
    "jest-when": "^3.5.2",
    "jira-prepare-commit-msg": "^1.7.1",
    "lint-staged": "^13.0.3",
    "localtunnel": "^2.0.2",
    "nodemon": "^2.0.20",
    "pgtools": "^0.3.2",
    "supertest": "^6.3.1"
  },
  "_moduleAliases": {
    "@": "./",
    "@config": "./config.js",
    "@models": "./src/db/models/index.js",
    "@routes": "./src/routes",
    "@Cache": "./src/classes/cache.js"
  },
  "jira-prepare-commit-msg": {
    "messagePattern": "$J $M",
    "jiraTicketPattern": "([A-Z]+-\\d+)",
    "commentChar": "#",
    "isConventionalCommit": false,
    "allowEmptyCommitMessage": false
  }
}
